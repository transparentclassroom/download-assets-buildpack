#!/bin/sh
# bin/compile <build-dir> <cache-dir>

# fail fast
set -e

# debug
set -x

cd $1
BUILD_NUMBER=`curl https://s3.amazonaws.com/build.transparentclassroom.com/builds/BUILD_NUMBER`
curl https://s3.amazonaws.com/build.transparentclassroom.com/builds/$BUILD_NUMBER/webpack.tgz
tar xvf webpack.tgz

